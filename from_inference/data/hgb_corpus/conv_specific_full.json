        "regex": "^(?P<mention_type>.+?)\\.(?P<entity_type>.+?)\\.(?P<mention_subtype>.*?)(?P<other>(\\..+?)*)$"
    },
    "pro": {
        "type": "Reference",
        "regex": "^(?P<mention_type>.+?)\\.(?P<entity_type>.+?)\\.(?P<mention_subtype>.*?)(?P<other>(\\..+?)*)$"
    },
    "att": {
